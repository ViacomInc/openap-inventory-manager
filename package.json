{
  "name": "@viacomcbs/openap-inventory-manager",
  "version": "0.0.11",
  "description": "Open AP Inventory Management",
  "author": "ViacomCBS",
  "license": "Apache-2.0",
  "bin": {
    "openap-migrate": "./bin/migrate",
    "openap-migrate-manager": "./bin/migrate-manager.js"
  },
  "scripts": {
    "analyze-bundle": "env ANALYZE=true WEBPACK_STATS=true next build",
    "prebuild": "rimraf ./@viacomcbs",
    "build": "npm run generate-types && rollup -c",
    "compile": "tsc --project tsconfig.json --noEmit",
    "compile:watch": "npm run compile -- --watch",
    "generate-types": "graphql-codegen --config codegen.yml",
    "lint": "eslint .",
    "test": "NODE_ENV=test npm run generate-types && jest --verbose --runInBand",
    "test:coverage": "NODE_ENV=test jest --runInBand --verbose --coverage",
    "test:watch": "NODE_ENV=test jest --verbose --watchAll --runInBand",
    "prepare": "husky install",
    "migrate": "./bin/migrate-manager.js",
    "format": "prettier --loglevel error --write \"./types/**/*.ts\" \"./tests/**/*.ts\" \"./src/**/*.ts\" \"./src/**/*.tsx\" \"./migrations/*.js\" \"*.md\"",
    "format:staged": "git-format-staged -f 'prettier --ignore-unknown --loglevel error --stdin --stdin-filepath \"{}\"' .",
    "dev:package:react": "cd @viacomcbs/openap-inventory-manager-react && npm pack && mv viacomcbs-openap-inventory-manager-react-*.tgz openap-inventory-manager-react-latest.tgz",
    "dev:package:server": "cd @viacomcbs/openap-inventory-manager-server && npm pack && mv viacomcbs-openap-inventory-manager-server-*.tgz openap-inventory-manager-server-latest.tgz",
    "dev:package": "npm run build && npm run dev:package:react && npm run dev:package:server",
    "license-check:prod": "license-checker --production",
    "license-check:prod:summary": "license-checker --production --summary",
    "license-check:prod:unknown": "license-checker --production --onlyunknown --summary"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ViacomInc/openap-inventory-manager.git"
  },
  "sideEffects": false,
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.15.0",
    "@viacomcbs/broadcast-calendar": "^1.2.0",
    "classnames": "^2.3.1",
    "debounce": "^1.2.1",
    "form-urlencoded": "^6.0.0",
    "fp-ts": "^2.10.0",
    "got": "^11.8.2",
    "graphile-worker": "^0.11.0",
    "graphql-request": "^3.4.0",
    "io-ts": "2.2.16",
    "io-ts-types": "^0.5.16",
    "luxon": "^2.0.0",
    "node-pg-migrate": "5.10.0",
    "papaparse": "5.3.1",
    "path-to-regexp": "^6.2.0",
    "pg": "^8.6.0",
    "pino": "^6.11.3",
    "pino-pretty": "^5.1.0",
    "query-string": "^7.0.0",
    "react-modal": "^3.12.1",
    "react-select": "^4.2.1",
    "react-spring": "^9.1.0",
    "react-table": "^7.6.2",
    "react-tabs": "^3.2.1",
    "slonik": "^24.0.0",
    "slonik-interceptor-query-logging": "^1.3.9",
    "use-timeout": "^1.1.0",
    "uuid": "^8.3.2",
    "yargs": "^17.1.0"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "^1.5.1",
    "apollo-server-micro": "^3.1.2",
    "graphql": "^15.5.0",
    "next": "^11.1.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.0"
  },
  "peerDependenciesServer": {
    "apollo-server-micro": "^3.1.2",
    "micro": "^9.3.4",
    "next": "^11.1.0",
    "react-dom": "^17.0.0",
    "graphql": "^15.5.0"
  },
  "peerDependenciesClient": {
    "@reduxjs/toolkit": "^1.5.1",
    "next": "^11.1.0",
    "react": "^17.0.0",
    "react-redux": "^7.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.0.0",
    "@graphql-codegen/introspection": "^2.0.0",
    "@graphql-codegen/schema-ast": "^2.0.0",
    "@graphql-codegen/typescript": "^2.0.0",
    "@graphql-codegen/typescript-graphql-files-modules": "^2.0.0",
    "@graphql-codegen/typescript-operations": "^2.0.0",
    "@graphql-codegen/typescript-resolvers": "^2.0.0",
    "@next/bundle-analyzer": "^11.1.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-graphql": "^1.0.0",
    "@rollup/plugin-inject": "^4.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/debounce": "^1.2.0",
    "@types/dotenv-flow": "3.1.1",
    "@types/faker": "^5.1.7",
    "@types/form-urlencoded": "^4.0.0",
    "@types/jest": "^27.0.0",
    "@types/luxon": "^2.0.0",
    "@types/node": "^16.0.0",
    "@types/papaparse": "^5.2.5",
    "@types/pino": "^6.3.7",
    "@types/react": "^17.0.17",
    "@types/react-modal": "^3.12.0",
    "@types/react-redux": "^7.1.18",
    "@types/react-select": "^4.0.13",
    "@types/react-table": "^7.7.2",
    "@types/react-tabs": "^2.3.3",
    "@types/slonik": "^22.1.5",
    "@types/uuid": "^8.3.1",
    "@types/yargs": "17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "apollo-server-testing": "^2.23.0",
    "autoprefixer": "^10.2.5",
    "dotenv-flow": "3.2.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.0",
    "factory.ts": "^0.5.2",
    "faker": "^5.4.0",
    "git-format-staged": "^2.1.2",
    "global": "^4.4.0",
    "graphql-tools": "^8.1.0",
    "husky": "^7.0.0",
    "jest": "^27.0.0",
    "jest-transform-graphql": "2.1.0",
    "license-checker": "25.0.1",
    "nock": "^13.1.1",
    "postcss": "^8.2.10",
    "postcss-modules": "^4.0.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.42.3",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-generate-package-json": "3.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-preserve-shebang": "1.0.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.3"
  }
}
