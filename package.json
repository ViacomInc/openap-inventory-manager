{
  "name": "openap-inventory-manager",
  "version": "0.0.10",
  "description": "Open AP Inventory Management",
  "author": "ViacomCBS",
  "license": "Apache-2.0",
  "bin": {
    "openap-migrate": "./bin/migrate",
    "openap-migration-manager": "./bin/migrate-manager.js"
  },
  "scripts": {
    "analyze-bundle": "env ANALYZE=true WEBPACK_STATS=true next build",
    "prebuild": "rimraf ./openap-inventory-manager-react ./openap-inventory-manager-server",
    "build": "npm run clean:build && npm run generate-types && rollup -c",
    "compile": "tsc --project tsconfig.json --noEmit",
    "compile:watch": "npm run compile -- --watch",
    "generate-types": "graphql-codegen --config codegen.yml",
    "lint": "eslint .",
    "test": "NODE_ENV=test npm run generate-types && jest --verbose --runInBand",
    "test:coverage": "NODE_ENV=test jest --runInBand --verbose --coverage",
    "test:watch": "NODE_ENV=test jest --verbose --watchAll --runInBand",
    "prepare": "husky install",
    "migrate": "./bin/migrate-manager.js",
    "format": "prettier --loglevel error --write \"./types/**/*.ts\" \"./tests/**/*.ts\" \"./src/**/*.ts\" \"./src/**/*.tsx\" \"./migrations/*.js\" \"*.md\"",
    "format:staged": "git-format-staged -f 'prettier --ignore-unknown --loglevel error --stdin --stdin-filepath \"{}\"' .",
    "dev:package:react": "cd openap-inventory-manager-react && npm pack && mv openap-inventory-manager-react-*.tgz openap-inventory-manager-react-latest.tgz",
    "dev:package:server": "cd openap-inventory-manager-server && npm pack && mv openap-inventory-manager-server-*.tgz openap-inventory-manager-server-latest.tgz",
    "dev:package": "npm run build && npm run dev:package:react && npm run dev:package:server"
  },
  "sideEffects": false,
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.15.0",
    "broadcast-calendar": "git+ssh://git@stash.mtvi.com/vaa/broadcast-calendar.git",
    "classnames": "^2.3.1",
    "debounce": "^1.2.1",
    "form-urlencoded": "^6.0.0",
    "fp-ts": "^2.10.0",
    "got": "^11.8.2",
    "graphile-worker": "^0.9.0",
    "graphql-request": "^3.4.0",
    "io-ts": "2.2.16",
    "io-ts-types": "^0.5.16",
    "luxon": "^1.26.0",
    "node-pg-migrate": "5.9.0",
    "papaparse": "5.3.0",
    "path-to-regexp": "^6.2.0",
    "pg": "^8.6.0",
    "pino": "^6.11.3",
    "pino-pretty": "^4.7.1",
    "query-string": "^7.0.0",
    "react-modal": "^3.12.1",
    "react-select": "^4.2.1",
    "react-spring": "^9.1.0",
    "react-table": "^7.6.2",
    "react-tabs": "^3.2.1",
    "slonik": "^23.5.5",
    "slonik-interceptor-query-logging": "^1.3.9",
    "use-timeout": "^1.1.0",
    "uuid": "^8.3.2",
    "yargs": "16.2.0"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "^1.5.1",
    "apollo-server-micro": "^2.23.0",
    "graphql": "^15.5.0",
    "next": "^10.1.3",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.0"
  },
  "peerDependenciesServer": {
    "apollo-server-micro": "^2.23.0",
    "next": "^10.1.3",
    "react-dom": "^17.0.0",
    "graphql": "^15.5.0"
  },
  "peerDependenciesClient": {
    "@reduxjs/toolkit": "^1.5.1",
    "next": "^10.1.3",
    "react": "^17.0.0",
    "react-redux": "^7.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/schema-ast": "^1.18.1",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-graphql-files-modules": "1.18.1",
    "@graphql-codegen/typescript-operations": "1.17.15",
    "@graphql-codegen/typescript-resolvers": "1.19.0",
    "@next/bundle-analyzer": "^10.1.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-graphql": "^1.0.0",
    "@rollup/plugin-inject": "^4.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@types/classnames": "^2.2.10",
    "@types/debounce": "^1.2.0",
    "@types/dotenv-flow": "3.1.0",
    "@types/faker": "^5.1.7",
    "@types/form-urlencoded": "^2.0.1",
    "@types/jest": "^26.0.22",
    "@types/luxon": "^1.26.2",
    "@types/node": "^14.14.40",
    "@types/papaparse": "^5.2.5",
    "@types/pino": "^6.3.7",
    "@types/react": "17.0.3",
    "@types/react-modal": "^3.12.0",
    "@types/react-redux": "7.1.16",
    "@types/react-select": "^4.0.13",
    "@types/react-table": "7.0.29",
    "@types/react-tabs": "2.3.2",
    "@types/slonik": "22.1.4",
    "@types/uuid": "8.3.0",
    "@types/yargs": "15.0.13",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "apollo-server-testing": "^2.23.0",
    "autoprefixer": "^10.2.5",
    "dotenv-flow": "3.2.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.0",
    "factory.ts": "0.5.1",
    "faker": "^5.4.0",
    "git-format-staged": "2.1.1",
    "global": "^4.4.0",
    "graphql-tools": "^7.0.4",
    "husky": "^6.0.0",
    "jest": "26.6.3",
    "jest-transform-graphql": "2.1.0",
    "nock": "13.0.11",
    "postcss": "^8.2.10",
    "postcss-modules": "^4.0.0",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.42.3",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-generate-package-json": "3.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-preserve-shebang": "1.0.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.0.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/tests"
    ],
    "transform": {
      "^.+\\.(gql|graphql)$": "jest-transform-graphql"
    },
    "setupFiles": [
      "<rootDir>/tests/setup-env.ts"
    ],
    "testEnvironment": "node",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.jest.json"
      }
    }
  }
}
